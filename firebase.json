{
  "hosting": {
    "public": "dist",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [{
      "source": "**",
      "destination": "/404.html",
      "type": 404
    },
    {
      "source": "/:path*",
      "destination": "https://milchchan.com/:path",
      "type": 301
    },
    {
      "source": "/",
      "destination": "https://milchchan.com/",
      "type": 301
    }]
  }
}
