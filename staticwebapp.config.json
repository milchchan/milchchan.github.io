{
    "$schema": "https://json.schemastore.org/staticwebapp.config.json",
    "routes": [
        {
          "route": "https://www.milchchan.com/*",
          "redirect": "https://milchchan.com/",
          "statusCode": 301
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    },
    "mimeTypes": {
        ".xml": "application/xml; charset=utf-8",
        ".txt": "text/plain",
        ".json": "text/json"
    }
}