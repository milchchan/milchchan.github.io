{
    "$schema": "https://json.schemastore.org/staticwebapp.config.json",
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    },
    "mimeTypes": {
        ".json": "text/json"
    }
}