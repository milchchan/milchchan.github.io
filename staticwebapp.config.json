{
    "$schema": "https://json.schemastore.org/staticwebapp.config.json",
    "routes": [
        {
            "route": "/privacy",
            "rewrite": "/privacy.html"
        },
        {
            "route": "/preview",
            "rewrite": "/preview/index.html"
        }
    ],
    "navigationFallback": {
        "rewrite": "index.html",
        "exclude": [
            "/images/*.{png,jpg,gif}"
        ]
    },
    "responseOverrides": {
        "404": {
            "rewrite": "/404.html"
        }
    },
    "mimeTypes": {
        ".json": "application/json"
    }
}